.nadata {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  font-size: 32rpx;
  height: 100vh;
  justify-content: center;
  width: 100%;
}
.nadata image {
  width: 80%;
}
.topserch {
  align-items: center;
  background-color: #004097;
  display: -webkit-flex;
  display: flex;
  font-size: 28rpx;
  height: 88rpx;
  justify-content: space-around;
  width: 100%;
}
.searchicon {
  left: 30rpx;
  position: absolute;
}
.topserch input[type="text"] {
  background-color: #fff;
  border-radius: 8rpx;
  box-sizing: border-box;
  height: 64rpx;
  padding-left: 58rpx;
  width: 566rpx;
}
button {
  height: 64rpx;
  line-height: 64rpx;
}
.selectcon {
  background-color: #fff;
  display: -webkit-flex;
  display: flex;
  height: 84rpx;
}
.selectcon picker {
  font-size: 32rpx;
  line-height: 84rpx;
  text-align: center;
  width: 50%;
}
.bridgedata {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  margin-bottom: 32rpx;
  width: 100%;
}
.bridgeli {
  background-color: #fff;
  border-radius: 16rpx;
  margin-top: 20rpx;
  width: 702rpx;
}
.bridgeid {
  overflow-x: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 80%;
}
.bridgeli .pass {
  background-color: #e1f9f1;
  color: #0e9474;
}
.bridgeli .pass,
.bridgeli .wating {
  border-radius: 8rpx;
  font-size: 24rpx;
  padding: 6rpx 18rpx;
}
.bridgeli .wating {
  background-color: #ffeee0;
  color: #ff7402;
}
.bridgeli .fail {
  background-color: #ffdbdf;
  color: #d81e36;
}
.bridgeli .fail,
.bridgeli .over {
  border-radius: 8rpx;
  font-size: 24rpx;
  padding: 6rpx 18rpx;
}
.bridgeli .over {
  background-color: #afd8ff;
  color: #004097;
}
.bridgelitop {
  color: #666;
  display: -webkit-flex;
  display: flex;
  justify-content: space-between;
  padding: 18rpx 32rpx;
}
.bridgelibow,
.bridgelitop {
  box-sizing: border-box;
  font-size: 32rpx;
}
.bridgelibow {
  line-height: 68rpx;
  padding: 28rpx 24rpx;
}
.bridgelibow .label {
  color: #999;
  margin-right: 32rpx;
}
.logistics {
  background: #004097;
  border-radius: 8rpx;
  color: #fff;
  font-size: 28rpx;
  line-height: 80rpx;
}
.bridgebtn {
  box-sizing: border-box;
  display: -webkit-flex;
  display: flex;
  justify-content: flex-end;
  padding-bottom: 25rpx;
  padding-right: 32rpx;
  width: 100%;
}
.ercamer {
  bottom: 134rpx;
  height: 136rpx;
  position: fixed;
  right: 0;
  width: 136rpx;
}
