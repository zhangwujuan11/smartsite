.news {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  margin-top: 24rpx;
  width: 100%;
}
.news_obj {
  height: 100rpx;
}
.page-body {
  display: -webkit-flex;
  display: flex;
  flex: 1;
  flex-direction: column;
  height: 150rpx;
  width: 100%;
}
.item {
  background-color: #fff;
  border-radius: 12rpx;
  box-sizing: border-box;
  margin: 10rpx auto 24rpx;
  padding: 11rpx 20rpx;
}
.page-header {
  border-bottom: 1rpx solid #ccc;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
}
.page-title {
  color: #999;
  font-size: 38rpx;
  padding: 20rpx 40rpx;
  text-align: center;
}
.item {
  align-items: center;
  border-bottom: 1rpx solid #eee;
  cursor: pointer;
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.item .bgImg {
  border-radius: 15rpx;
  height: 150rpx;
  margin-right: 30rpx;
  position: relative;
  width: 306rpx;
  z-index: 1;
  z-index: 40;
}
.item .bgImg .posterb {
  background-color: #efefef;
  filter: blur(3px);
  height: 150rpx;
  margin-right: 10rpx;
  position: absolute;
  width: 306rpx;
  z-index: 10;
}
.item .bgImg .poster {
  height: 150rpx;
  margin: 0 72rpx;
  position: absolute;
  width: 162rpx;
  z-index: 40;
}
.item .meta {
  flex: 1;
  height: 120rpx;
  position: relative;
}
.item .meta .ti tle {
  overflow: hidden;
}
.item .meta .sub-title {
  bottom: 0;
  color: #b9c2d3;
  display: -webkit-flex;
  display: flex;
  font-size: 24rpx;
  position: absolute;
  text-align: left;
}
.item .meta .artists {
  color: #999;
  font-size: 26rpx;
}
.item .rating {
  color: #f74c31;
  font-size: 28rpx;
  font-weight: 700;
}
.tips {
  color: #ccc;
  font-size: 28rpx;
  padding: 50rpx;
  text-align: center;
}
.tips image {
  height: 40rpx;
  margin-right: 20rpx;
  width: 40rpx;
}
.tips image,
.tips text {
  vertical-align: middle;
}
.page-section-gap {
  box-sizing: border-box;
  padding: 0 30rpx;
}
.page-body-button {
  margin-bottom: 30rpx;
}
.loading {
  padding: 10rpx;
  text-align: center;
}
.loading:before {
  animation: rotate 1s linear infinite;
  background: url(data:image/png;base64,)
    no-repeat;
  background-size: contain;
  content: "";
  display: inline-block;
  height: 40rpx;
  margin-right: 5rpx;
  vertical-align: middle;
  width: 40rpx;
}
.loading.complete:before {
  display: none;
}
