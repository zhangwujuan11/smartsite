.news {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top: 20rpx;
  padding: 0rpx 24rpx;
}
.box-rank-number {
  background-color: #ddf4ea;
  border-radius: 30rpx;
  color: #89dfb2;
  font-size: 16rpx;
  height: 30rpx;
  line-height: 30rpx;
  padding: 2rpx 8rpx;
  position: absolute;
  right: 7rpx;
  text-align: center;
}
.rank-bg {
  background: linear-gradient(to bottom right, #fecdd3, #fecdd3);
  color: #fb354e;
}
.searchbox {
  padding-top: 64rpx;
}
.page-header {
  border-bottom: 1rpx solid #ccc;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
}
.page-title {
  color: #999;
  font-size: 38rpx;
  padding: 20rpx 40rpx;
  text-align: center;
}
.page-body {
  flex-direction: column;
  -webkit-flex-direction: row;
  flex-direction: row;
  margin: 15rpx 8rpx;
  overflow: hidden;
}
.item,
.page-body {
  background-color: #fff;
  border-radius: 20rpx;
  display: -webkit-flex;
  display: flex;
}
.item {
  border-bottom: 1rpx solid #f3f3f3;
  cursor: pointer;
  flex-direction: column;
  height: 180rpx;
  width: 330rpx;
}
.item .poster {
  border-radius: 20rpx 0 0 0;
  height: 240rpx;
  width: 330rpx;
}
.item .meta {
  margin-bottom: 10rpx;
  padding: 8rpx;
}
.item .meta .title {
  display: block;
  font-size: 28rpx;
  margin: 19rpx 4rpx;
  width: 100%;
}
.item .meta .title,
.item .meta .titles {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.item .meta .titles {
  color: silver;
  font-size: 26rpx;
  margin-left: 7rpx;
  width: 200rpx;
}
.item .meta .sub-title {
  color: silver;
  display: -webkit-flex;
  display: flex;
  font-size: 30rpx;
  text-align: left;
}
.item .meta .artists {
  color: #999;
  font-size: 26rpx;
}
.item .rating {
  color: #f74c31;
  font-size: 28rpx;
  font-weight: 700;
}
.tips {
  color: #ccc;
  font-size: 28rpx;
  padding: 50rpx;
  text-align: center;
}
.tips image {
  height: 40rpx;
  margin-right: 20rpx;
  width: 40rpx;
}
.tips image,
.tips text {
  vertical-align: middle;
}
.page-section-gap {
  box-sizing: border-box;
  padding: 0 30rpx;
}
.page-body-button {
  margin-bottom: 30rpx;
}
.loading {
  padding: 10rpx;
  text-align: center;
}
.loading:before {
  animation: rotate 1s linear infinite;
  background: url(data:image/png;base64,)
    no-repeat;
  background-size: contain;
  content: "";
  display: inline-block;
  height: 40rpx;
  margin-right: 5rpx;
  vertical-align: middle;
  width: 40rpx;
}
.loading.complete:before {
  display: none;
}
.baise {
  background: #f4f7fc;
  bottom: 0rpx;
  height: 115rpx;
  z-index: 665;
}
.anniu,
.baise {
  display: -webkit-flex;
  display: flex;
  position: fixed;
  width: 100%;
}
.anniu {
  bottom: 7rpx;
  z-index: 666;
}
.btn {
  background-color: #ff9c22;
  border-radius: 50rpx;
  color: #fff;
  font-size: 32rpx;
  height: 75rpx;
  line-height: 75rpx;
  margin: 30rpx 20rpx 50rpx 30rpx;
  text-align: center;
  width: 690rpx;
}
.btbdstk {
  margin-left: 30rpx;
  width: 100%;
}
.bdstk {
  background: #78b8fd;
}
.bdstk,
.sqdzk {
  border: 2rpx solid #fff;
  border-radius: 15rpx;
  color: #fff;
  font-size: 30rpx;
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  width: 100%;
}
.sqdzk {
  background: #ff7178;
}
.monitor-tabbar {
  align-items: center;
  background-color: #fff;
  border-radius: 10rpx 10rpx 0 0;
  box-sizing: border-box;
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 0 116rpx;
  position: fixed;
  width: 750rpx;
  z-index: 33335;
}
.monitor-tab-item {
  color: #b9c2d3;
  font-size: 30rpx;
  padding-bottom: 14rpx;
  padding-top: 14rpx;
}
.on {
  align-items: center;
  color: #004998;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  font-weight: 700;
  justify-content: center;
  position: relative;
  transition: all 0.3s;
}
.on::after {
  background-color: #004998;
  border-radius: 10rpx;
  bottom: 0;
  content: "";
  display: block;
  height: 4rpx;
  position: absolute;
  width: 110rpx;
}
.pages_box {
  align-content: flex-start;
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 20rpx;
}
.pages_box > view {
  background-color: #dfdfdf;
  border-radius: 50%;
  color: #9f9f9f;
  font-size: 25rpx;
  height: 60rpx;
  margin: 0 8rpx;
  width: 60rpx;
}
.pages_box > view.active {
  background-color: #fc6143;
  color: #fff;
}
.pages_box > view:first-child,
.pages_box > view:last-child {
  border-radius: 60rpx;
}
.pages_box .page_next {
  background-color: #004998;
  color: #fff;
  font-size: 20rpx;
  margin-bottom: 30rpx;
  width: 130rpx;
}
.inputbx,
.pages_box .page_next {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
}
.inputbx {
  border-radius: 0rpx !important;
  width: -webkit-fit-content;
  width: fit-content;
  width: 180rpx !important;
}
.forminput {
  border: 1rpx solid #9f9f9f;
  width: 60rpx;
}
.chartbigbox,
ec-canvas {
  width: 100%;
}
ec-canvas {
  display: block;
  height: 300rpx;
}
.chartbigbox {
  position: relative;
}
.bxs {
  visibility: hidden;
}
