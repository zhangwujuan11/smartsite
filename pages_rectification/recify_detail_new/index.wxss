.filrico image {
  padding-right: 10rpx;
  vertical-align: middle;
  width: 50rpx;
}
.textbtn {
  color: #4467b1;
  margin: 0rpx 20rpx;
}
.file-item {
  background-color: #f1f1f1;
  border-bottom: 1rpx solid #e9e9e9;
  box-sizing: border-box;
  line-height: 80rpx;
  padding: 0rpx 20rpx;
}
.img-add,
.img-item {
  display: inline-block;
  width: -webkit-fit-content;
  width: fit-content;
}
.zgbtn {
  border: 1rpx solid #d3d3d3;
  border-radius: 6rpx;
  color: #333;
  font-size: 20rpx;
  margin: 16rpx 20rpx 16rpx 0rpx;
  padding: 8rpx 18rpx;
}
.acbtn {
  background: #f9fdff;
  border: 1rpx solid #2568c0;
  color: #2e5aaa;
}
textarea {
  border: 1px solid #d3d3d3;
  border-radius: 10rpx;
  box-sizing: border-box;
  line-height: 40rpx;
  padding-left: 20rpx;
  padding-top: 30rpx;
  width: 100%;
}
.img-upload {
  padding-bottom: 10rpx;
  padding-top: 20rpx;
}
.img-upload image {
  display: block;
  height: 180rpx;
  width: 180rpx;
}
.file-add {
  color: #333;
}
.addbtn {
  border: 4rpx dashed #417fcf;
  border-radius: 10rpx;
  color: #316fcc;
  line-height: 100rpx;
  margin: 10rpx 0;
  text-align: center;
}
.formbottm {
  padding-top: 20rpx;
}
.formbtn {
  background: #fff;
  border-radius: 6rpx;
  flex: 1 1 auto;
  font-size: 26rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: 0rpx 10rpx;
  padding: 0rpx 20rpx;
  text-align: center;
}
.bgg {
  background: #409e34;
}
.bgb,
.bgg {
  color: #fff;
}
.bgb {
  background: #354edb;
}
.bgr {
  background: #d43b55;
}
.bgr,
.bgy {
  color: #fff;
}
.bgy {
  background: #f8b014;
}
.cartitle {
  font-size: 24rpx;
  line-height: 50rpx;
}
.labelbox {
  width: 120rpx;
}
.status {
  color: #07a70f;
  padding-right: 6rpx;
}
.titleata {
  font-size: 28rpx;
  font-weight: 700;
}
