.s_table {
  border-bottom: 0;
  display: -webkit-inline-flex;
  display: inline-flex;
  flex-direction: column;
  font-size: 30rpx;
  min-width: 100%;
}
.s_table-border {
  border: 2rpx solid #dfdfdf;
}
.s_table-header {
  align-items: stretch;
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  font-weight: 700;
  justify-content: flex-start;
}
.s_table-header-column {
  word-wrap: break-word;
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex-shrink: 0;
  justify-content: center;
  padding: 10rpx;
  word-break: break-all;
}
.last-column {
  flex-grow: 1;
}
.s_table-header-column-border {
  border-right: 2rpx solid #dfdfdf;
}
.s_table-header-column-border:last-child {
  border-right: 0rpx solid #dfdfdf;
}
.s_table-content-row {
  align-items: stretch;
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}
.s_table-row-white {
  background-color: #fff;
}
.s_table-row-gray {
  background-color: #efefef;
}
.s_table-content-column {
  word-wrap: break-word;
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex-shrink: 0;
  justify-content: center;
  overflow: hidden;
  padding: 10rpx;
  word-break: break-all;
}
.s_table-content-column-border {
  border-right: 2rpx solid #dfdfdf;
  border-top: 2rpx solid #dfdfdf;
}
.s_table-content-column-border:last-child {
  border-right: 0rpx solid #dfdfdf;
}
.nodata {
  color: #999;
  padding: 30rpx;
  text-align: center;
}
